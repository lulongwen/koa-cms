{
  "name": "koa-cms",
  "version": "1.0.0",
  "description": "nodejs koa定制化能力很强，`koa-cms`打造一个好用的异步编程模型 web框架",
  "main": "index.js",
  "scripts": {
    "start": "nodemon app.js",
    "dev": "cross-env NODE_ENV=dev",
    "prod": "cross-env NODE_ENV=production pm2 start app.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "koa2-cms",
    "koa2-blog",
    "koa2"
  ],
  "author": "www.lulongwen.com",
  "license": "ISC",
  "dependencies": {
    "@koa/router": "^9.4.0",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^5.0.0-alpha2",
    "ejs": "^3.1.5",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-nunjucks-2": "^3.0.2",
    "koa-router": "^8.0.8",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.20",
    "module-alias": "^2.2.2",
    "mysql2": "^2.2.5",
    "npm-check": "^5.9.2",
    "require-directory": "^2.1.1",
    "sequelize": "^5.22.3",
    "validator": "^13.7.0"
  },
  "_moduleAliases": {
    "@": ".",
    "@app": "app",
    "@src": "src",
    "@config": "config",
